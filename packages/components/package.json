{
  "name": "@diogobiz/components",
  "version": "1.0.17",
  "license": "ISC",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "yarn build:babel && yarn build:types",
    "build:babel": "cross-env BABEL_ENV=build babel src -d dist --source-maps --delete-dir-on-start --config-file ./babel.config.js --extensions \".ts,.tsx\" --no-comments --copy-files",
    "build:storybook": "build-storybook",
    "build:types": "tsc -project tsconfig.json",
    "cli": "hygen sanar-components",
    "cli:component": "yarn cli",
    "start": "yarn build -w",
    "start:dev": "npm-run-all --parallel start build:types",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "rollup": "rollup -c ../../rollup.config.js"
  },
  "peerDependencies": {
    "@diogobiz/sanar-ui": "*",
    "@diogobiz/utils": "*"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.4",
    "@fullcalendar/core": "^4.3.1",
    "@fullcalendar/daygrid": "^4.3.0",
    "@fullcalendar/interaction": "^4.3.0",
    "@fullcalendar/react": "^4.3.0",
    "@opuscapita/react-perfect-scrollbar": "^3.0.4",
    "apollo-client": "^2.6.8",
    "date-fns": "^2.5.1",
    "i18next": "^19.4.1",
    "i18next-browser-languagedetector": "^4.0.2",
    "less": "2.7.2",
    "less-plugin-npm-import": "2.1.0",
    "react": "^16.8.6",
    "react-compound-slider": "^2.3.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.8.6",
    "react-eva-icons": "^0.0.8",
    "react-focus-lock": "^2.1.0",
    "react-i18next": "^11.3.4",
    "react-infinite-scroller": "^1.2.4",
    "react-input-mask": "^2.0.4",
    "react-linkify": "^1.0.0-alpha",
    "react-remove-scroll": "^2.0.4",
    "react-router-dom": "5.1.2",
    "react-slick": "^0.25.2",
    "react-snackbar-alert": "^2.1.0",
    "react-tooltip": "^3.11.1",
